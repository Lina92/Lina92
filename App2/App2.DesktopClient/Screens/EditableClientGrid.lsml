            Kind="Collection"
            DataType="CollectionView$ClientItem">
            <ContentItem.DataSource>
              <ScreenExpressionTree>
                <ChainExpression>
                  <MemberExpression
                    Member="EditableClientGrid/Properties[Client]" />
                </ChainExpression>
              </ScreenExpressionTree>
            </ContentItem.DataSource>
            <ContentItem
              Name="RowTemplate"
              Kind="Details"
              DataType="ClientItem">
              <ContentItem.DataSource>
                <ScreenExpressionTree>
